{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "setFilePermissions": {
      "handler": "api/set_file_permissions.set_file_permissions",
      "events": [
        {
          "http": {
            "method": "post",
            "path": "/api/set-file-permissions"
          }
        }
      ]
    }
  }
}